# 数据库配置
DATABASE_TYPE=mysql
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=root
MYSQL_USER=root
MYSQL_PASSWORD=

# DashScope API配置
DASHSCOPE_API_KEY=
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# 模型配置
LLM_MODEL_NAME=qwen-max
LLM_CODER_MODEL=qwen-coder-plus
EMBEDDING_MODEL=text-embedding-v3
RERANK_MODEL=gte-rerank-v2


# 缓存和存储配置
KNOWLEDGE_BASE_PATH=./data/knowledge_base
SCHEMA_CACHE_TTL=3600

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=./logs/chatbi.log

# RAG配置
RAG_ENABLED=true
RAG_SIMILARITY_THRESHOLD=0.6
RAG_CONFIDENCE_THRESHOLD=0.8
RAG_MAX_EXAMPLES=3
RAG_VECTOR_DIMENSION=1024
RAG_SEARCH_TIMEOUT=5.0
RAG_CACHE_SIZE=1000
RAG_BATCH_SIZE=100
RAG_INDEX_TYPE=hnsw
RAG_DISTANCE_METRIC=cosine
